{
	"name": "pico-artist-website-v2",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=20.0.0 <21"
	},
	"scripts": {
		"dev": "next dev --turbopack",
		"prebuild": "npm run lint && npm run type-check || true",
		"build": "node scripts/next-build.mjs",
		"cf:build": "opennextjs-cloudflare build && npm run verify:output",
		"build:sanity": "node ./scripts/cloudflare-sanity-check.js",
		"sync:env": "node ./scripts/env-sync.js",
		"start": "next start",
		"lint": "next lint",
		"type-check": "tsc --noEmit",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"verify:output": "node scripts/verify-output.mjs",
		"prepare": "node -e \"try{require('child_process').execSync('husky install',{stdio:'inherit'})}catch(e){process.exit(0)}\""
	},
	"dependencies": {
		"@opennextjs/cloudflare": "1.14.4",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-slot": "^1.2.4",
		"@react-three/drei": "^10.7.7",
		"@react-three/fiber": "^9.4.2",
		"@supabase/supabase-js": "^2.89.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"framer-motion": "^12.23.26",
		"lenis": "^1.3.16",
		"lucide-react": "^0.562.0",
		"motion": "^12.23.26",
		"next": "15.5.9",
		"openai": "^6.15.0",
		"react": "19.1.4",
		"react-dom": "19.1.4",
		"react-hook-form": "^7.53.0",
		"tailwind-merge": "^3.4.0",
		"tailwindcss-animate": "^1.0.7",
		"three": "^0.182.0",
		"wavesurfer.js": "^7.12.1",
		"zod": "3.25.76",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@types/node": "^20.19.27",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/wavesurfer.js": "^6.0.12",
		"autoprefixer": "10.4.23",
		"eslint": "^9",
		"eslint-config-next": "15.5.9",
		"husky": "8.0.3",
		"lint-staged": "15.2.10",
		"postcss": "8.4.49",
		"tailwindcss": "3.4.14",
		"typescript": "5.9.3",
		"wrangler": "^4.56.0"
	}
}
